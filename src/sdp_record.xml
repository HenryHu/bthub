<?xml version="1.0" encoding="UTF-8" ?>

<record>
    <!-- Service Class ID -->
    <attribute id="0x0001">
        <sequence>
            <!-- HID -->
            <uuid value="0x1124" />
        </sequence>
    </attribute>
    <!-- Protocol Descriptor list -->
    <attribute id="0x0004">
        <sequence>
            <sequence>
                <!-- L2CAP -->
                <uuid value="0x0100" />
                <!-- HID Control -->
                <uint16 value="0x0011" />
            </sequence>
            <sequence>
                <!-- HID Protocol -->
                <uuid value="0x0011" />
            </sequence>
        </sequence>
    </attribute>
    <attribute id="0x0005">
        <sequence>
            <uuid value="0x1002" />
        </sequence>
    </attribute>
    <!-- LanguageBaseAttribute ID list -->
    <attribute id="0x0006">
        <sequence>
            <uint16 value="0x656e" />
            <uint16 value="0x006a" />
            <uint16 value="0x0100" />
        </sequence>
    </attribute>
    <!-- Bluetooth Profile descriptor list -->
    <attribute id="0x0009">
        <sequence>
            <sequence>
                <!-- HID profile -->
                <uuid value="0x1124" />
                <!-- Version 1.00 -->
                <uint16 value="0x0100" />
            </sequence>
        </sequence>
    </attribute>
    <!-- Additional Protocol Descriptor Lists -->
    <attribute id="0x000d">
        <sequence>
            <sequence>
                <sequence>
                    <!-- L2CAP -->
                    <uuid value="0x0100" />
                    <!-- HID Interrupt -->
                    <uint16 value="0x0013" />
                </sequence>
                <sequence>
                    <!-- HID Protocol -->
                    <uuid value="0x0011" />
                </sequence>
            </sequence>
        </sequence>
    </attribute>
    <!-- Service name -->
    <attribute id="0x0100">
        <text value="bthub Virtual Keyboard" />
    </attribute>
    <!-- Service description -->
    <attribute id="0x0101">
        <text value="virtual bluetooth keyboard" />
    </attribute>
    <!-- Provider name -->
    <attribute id="0x0102">
        <text value="bthub project" />
    </attribute>
    <!-- HID device release number, deprecated -->
    <attribute id="0x0200">
        <uint16 value="0x0100" />
    </attribute>
    <!-- HID Parser version -->
    <attribute id="0x0201">
        <uint16 value="0x0111" />
    </attribute>
    <!-- HID device subclass -->
    <attribute id="0x0202">
        <!--
            0xc0: mouse + keyboard
        -->
        <uint8 value="0xc0" />
    </attribute>
    <!-- HID country code -->
    <attribute id="0x0203">
        <!-- not localized -->
        <uint8 value="0x00" />
    </attribute>
    <!-- HID virtual cable -->
    <attribute id="0x0204">
        <boolean value="false" />
    </attribute>
    <!-- HID Reconnect initiate -->
    <attribute id="0x0205">
        <boolean value="false" />
    </attribute>
    <!-- HID descriptor list -->
    <attribute id="0x0206">
        <sequence>
            <sequence>
                <!-- HID report descriptor -->
                <uint8 value="0x22" />
                <text encoding="hex" value="" />
            </sequence>
        </sequence>
    </attribute>
    <!-- HID Language ID Base list -->
    <attribute id="0x0207">
        <sequence>
            <sequence>
                <uint16 value="0x0409" />
                <uint16 value="0x0100" />
            </sequence>
        </sequence>
    </attribute>
    <!-- HID profile version, deprecated -->
    <attribute id="0x020b">
        <uint16 value="0x0100" />
    </attribute>
    <!-- HID Supervision timeout -->
    <attribute id="0x020c">
        <uint16 value="0x0c80" />
    </attribute>
    <!-- HID normally connectable -->
    <attribute id="0x020d">
        <boolean value="true" />
    </attribute>
    <!-- HID boot device -->
    <attribute id="0x020e">
        <boolean value="false" />
    </attribute>
    <!-- HID SSR host max latency -->
    <attribute id="0x020f">
        <uint16 value="0x0640" />
    </attribute>
    <!-- HID SSR host min latency -->
    <attribute id="0x0210">
        <uint16 value="0x0320" />
    </attribute>
</record>
